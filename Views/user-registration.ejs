<%- contentFor('HeaderCss') %>
    <%- contentFor('body') %>

        <div class="row">
            <div class="col-xl-6">
                <div class="card">
                    <div class="card-body">
                        <form action="#" class="form-steps" autocomplete="off">
                            <div class="text-center pt-3 pb-4 mb-1">
                                <img src="assets/images/logo-dark.png" alt="" height="17">
                            </div>
                            <div class="step-arrow-nav mb-4">

                                <ul class="nav nav-pills custom-nav nav-justified" role="tablist">
                                    <li class="nav-item" role="presentation">
                                        <button class="nav-link done" id="steparrow-personal-info-tab" data-bs-toggle="pill"
                                            data-bs-target="#steparrow-personal-info" type="button" role="tab"
                                            aria-controls="steparrow-personal-info" aria-selected="false">Personal Info</button>
                                    </li>
                                    <li class="nav-item" role="presentation">
                                        <button class="nav-link active" id="steparrow-location-info-tab"
                                            data-bs-toggle="pill" data-bs-target="#steparrow-location-info"
                                            type="button" role="tab" aria-controls="steparrow-location-info"
                                            aria-selected="false">Location Info</button>
                                    </li>
									<li class="nav-item" role="presentation">
                                        <button class="nav-link active" id="steparrow-location-info-tab"
                                            data-bs-toggle="pill" data-bs-target="#steparrow-additional-info"
                                            type="button" role="tab" aria-controls="steparrow-additional-info"
                                            aria-selected="false">Additional Info</button>
                                    </li>
                                    <li class="nav-item" role="presentation">
                                        <button class="nav-link" id="pills-experience-tab" data-bs-toggle="pill"
                                            data-bs-target="#pills-experience" type="button" role="tab"
                                            aria-controls="pills-experience" aria-selected="false">Finish</button>
                                    </li>
                                </ul>
                            </div>

                            <div class="tab-content">
                                <div class="tab-pane fade" id="steparrow-personal-info" role="tabpanel"
                                    aria-labelledby="steparrow-personal-info-tab">
                                    <div>
                                        <div class="row">
                                            <div class="col-lg-6">
                                                <div class="mb-3">
                                                    <label class="form-label"
                                                        for="steparrow-personal-info-firstname-input">First Name</label>
                                                    <input type="text" class="form-control"
                                                        id="steparrow-personal-info-firstname-input" placeholder="Enter your first name"
                                                        required>
                                                    <div class="invalid-feedback">Please enter your first name</div>
                                                </div>
												<div class="mb-3">
                                                    <label class="form-label"
                                                        for="steparrow-personal-info-lastname-input">Last Name</label>
                                                    <input type="text" class="form-control"
                                                        id="steparrow-personal-info-lastname-input" placeholder="Enter enter your last namel"
                                                        required>
                                                    <div class="invalid-feedback">Please enter your last name</div>
                                                </div>
												<div class="mb-3">
                                                    <label class="form-label"
                                                        for="steparrow-personal-info-email-input">Email</label>
                                                    <input type="email" class="form-control"
                                                        id="steparrow-personal-info-email-input" placeholder="Enter email"
                                                        required>
                                                    <div class="invalid-feedback">Please enter an email address</div>
                                                </div>
                                            </div>
                                            <div class="col-lg-6">
                                                <div class="mb-3">
                                                    <label class="form-label"
                                                        for="steparrow-personal-info-username-input">User Name</label>
                                                    <input type="text" class="form-control"
                                                        id="steparrow-personal-info-username-input"
                                                        placeholder="Enter user name" required>
                                                    <div class="invalid-feedback">Please enter a user name</div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="mb-3">
                                            <label class="form-label"
                                                for="steparrow-personal-info-password-input">Password</label>
                                            <input type="password" class="form-control"
                                                id="steparrow-personal-info-password-input" placeholder="Enter password"
                                                required>
                                            <div class="invalid-feedback">Please enter a password</div>
                                        </div>
                                        <div>
                                            <label class="form-label"
                                                for="steparrow-personal-info-confirm-password-input">Confirm Password</label>
                                            <input type="password" class="form-control"
                                                id="steparrow-personal-info-confirm-password-input"
                                                placeholder="Enter confirm password" required>
                                            <div class="invalid-feedback">Please enter a confirm password</div>
                                        </div>
                                    </div>
                                    <div class="d-flex align-items-start gap-3 mt-4">
                                        <button type="button"
                                            class="btn btn-success btn-label right ms-auto nexttab nexttab"
                                            data-nexttab="steparrow-location-info-tab"><i
                                                class="ri-arrow-right-line label-icon align-middle fs-16 ms-2"></i>Add your location information...</button>
                                    </div>
									
                                </div>
                                <!-- end tab pane -->
								
                                <div class="tab-pane fade show active" id="steparrow-location-info" role="tabpanel"
                                    aria-labelledby="steparrow-location-info-tab">
                                    <div>
                                        
                                        <div>
                                            <label class="form-label"
                                                for="des-info-description-input">Description</label>
                                            <textarea class="form-control" placeholder="Enter Description"
                                                id="des-info-description-input" rows="3" required></textarea>
                                            <div class="invalid-feedback">Please enter a description</div>
                                        </div>
                                    </div>
                                    <div class="d-flex align-items-start gap-3 mt-4">
                                        <button type="button" class="btn btn-light btn-label previestab"
                                            data-previous="steparrow-additional-info-tab"><i
                                                class="ri-arrow-left-line label-icon align-middle fs-16 me-2"></i> Back
                                            to General</button>
                                        <button type="button"
                                            class="btn btn-success btn-label right ms-auto nexttab nexttab"
                                            data-nexttab="pills-experience-tab"><i
                                                class="ri-arrow-right-line label-icon align-middle fs-16 ms-2"></i>Submit</button>
                                    </div>
                                </div>
                                <!-- end tab pane -->
								<div class="tab-content">
                                <div class="tab-pane fade" id="steparrow-additional-info" role="tabpanel"
                                    aria-labelledby="steparrow-additional-info-tab">
                                    <div>
                                        <div class="row">
                                            <div class="col-lg-6">
											<div class="mb-3">
												<label for="formFile" class="form-label">Upload Image</label>
												<input class="form-control" type="file" id="formFile" />
											</div>
                                                <div class="mb-3">
                                                    <label class="form-label"
                                                        for="steparrow-additional-info-email-input">Email</label>
                                                    <input type="email" class="form-control"
                                                        id="steparrow-additional-info-email-input" placeholder="Enter email"
                                                        required>
                                                    <div class="invalid-feedback">Please enter an email address</div>
                                                </div>
                                            </div>
                                            <div class="col-lg-6">
                                                <div class="mb-3">
                                                    <label class="form-label"
                                                        for="steparrow-additional-info-username-input">User Name</label>
                                                    <input type="text" class="form-control"
                                                        id="steparrow-additional-info-username-input"
                                                        placeholder="Enter user name" required>
                                                    <div class="invalid-feedback">Please enter a user name</div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="mb-3">
                                            <label class="form-label"
                                                for="steparrow-additional-info-password-input">Password</label>
                                            <input type="password" class="form-control"
                                                id="steparrow-additional-info-password-input" placeholder="Enter password"
                                                required>
                                            <div class="invalid-feedback">Please enter a password</div>
                                        </div>
                                        <div>
                                            <label class="form-label"
                                                for="steparrow-additional-info-confirm-password-input">Confirm Password</label>
                                            <input type="password" class="form-control"
                                                id="steparrow-additional-info-confirm-password-input"
                                                placeholder="Enter confirm password" required>
                                            <div class="invalid-feedback">Please enter a confirm password</div>
                                        </div>
                                    </div>
                                    <div class="d-flex align-items-start gap-3 mt-4">
                                        <button type="button"
                                            class="btn btn-success btn-label right ms-auto nexttab nexttab"
                                            data-nexttab="steparrow-location-info-tab"><i
                                                class="ri-arrow-right-line label-icon align-middle fs-16 ms-2"></i>Just a few more deatils...</button>
                                    </div>
									
                                </div>
                                <!-- end tab pane -->
                                <div class="tab-pane fade" id="pills-experience" role="tabpanel">
                                    <div class="text-center">

                                        <div class="avatar-md mt-5 mb-4 mx-auto">
                                            <div class="avatar-title bg-light text-success display-4 rounded-circle">
                                                <i class="ri-checkbox-circle-fill"></i>
                                            </div>
                                        </div>
                                        <h5>Well Done !</h5>
                                        <p class="text-muted">You have Successfully Signed Up</p>
                                    </div>
                                </div>
                                <!-- end tab pane -->
                            </div>
                            <!-- end tab content -->
                        </form>
                    </div>
                    <!-- end card body -->
                </div>
                <!-- end card -->
            </div>
            <!-- end col -->
        </div><!-- end row -->
        
        <!-- end row -->

        <%- contentFor('FooterJs') %>

            <!-- form wizard init -->
            <script src="assets/js/pages/form-wizard.init.js"></script>